{
  "name": "mongoose-graphql-codegen",
  "version": "1.2.2",
  "description": "Generate GraphQL schema and resolvers from Mongoose models (including BSON types)",
  "main": "./dist/cjs/bin/cli.js",
  "module": "./dist/esm/bin/cli.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    "import": "./dist/esm/bin/cli.js",
    "require": "./dist/cjs/bin/cli.js"
  },
  "bin": {
    "mongoose-graphql-codegen": "./dist/cjs/bin/cli.js"
  },
  "scripts": {
    "build:cjs": "tsc -p tsconfig.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build": "npm run build:cjs && npm run build:esm",
    "generate": "node dist/cjs/bin/cli.js",
    "generate:ts": "ts-node ./bin/cli.ts"
  },
  "keywords": [
    "mongoose",
    "graphql",
    "codegen",
    "resolver",
    "schema",
    "generator",
    "bson",
    "typescript"
  ],
  "contributors": [
    "Swetabh Suman (https://github.com/swtbhsmn)"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/swtbhsmn/mongoose-graphql-codegen"
  },
  "author": "Swetabh Suman",
  "license": "MIT",
  "dependencies": {
    "@graphql-tools/load-files": "^7.0.1",
    "@graphql-tools/merge": "^9.0.24",
    "@types/node": "^20.19.4",
    "@types/pluralize": "^0.0.33",
    "graphql": "^16.8.0",
    "graphql-scalars": "^1.22.5",
    "mongoose": "^7.6.1",
    "pluralize": "^8.0.0",
    "validator": "^13.9.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}